{
    "ruff.enable": true,
    "ruff.fixAll": false,
    "[python]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.codeActionsOnSave": {
            "source.organizeImports": "always",
            "source.fixAll": "explicit",
        }
    },
    "cSpell.words": [
        "cmdlen",
        "ctty",
        "curframe",
        "getval",
        "inspectall",
        "interactiveshell",
        "lasti",
        "madbg",
        "maxcol",
        "nspect",
        "pdef",
        "pdoc",
        "pinfo",
        "PLAND",
        "ptutils",
        "signum",
        "SIGWINCH",
        "tcdrain",
        "tcgetattr",
        "varstree",
        "ZhengYu"
    ],
}
